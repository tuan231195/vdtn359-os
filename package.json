{
  "name": "@vdtn359/os",
  "version": "0.0.0",
  "private": true,
  "description": "Tuan's open source project",
  "license": "ISC",
  "author": "Tuan Nguyen <vdtn359@gmail.com>",
  "scripts": {
    "dev": "chokidar --silent \"packages/*/src/**/*\" --initial -c \"time ts-transpile -b packages\"",
    "build": "tsc -b packages",
    "prepare": "npm run build",
    "postinstall": "link-parent-bin > /dev/null",
    "publish": "lerna publish",
    "publish:major": "lerna publish major",
    "publish:minor": "lerna publish minor",
    "publish:patch": "lerna publish patch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{json,html,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.2",
    "@zerollup/ts-transform-paths": "1.7.12",
    "lerna": "^3.20.2",
    "ts-jest": "25.2.1",
    "ts-transpile": "~1.2.4",
    "ttypescript": "1.5.10",
    "typescript": "3.8.3"
  },
  "optionalDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "chokidar-cli": "^2.1.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "link-parent-bin": "1.0.2",
    "lint-staged": "10.0.8",
    "prettier": "2.0.2"
  },
  "dependencies": {
    "@vdtn359/utils": "file:packages/utils"
  }
}
